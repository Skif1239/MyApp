<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="OblakoBar">
        <item name="android:textColor">@color/whiteOblako</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="OblakoAdress">
        <item name="android:textColor">@color/adressOblako</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">italic</item>
    </style>
    <style name="OblakoAdressSecond">
        <item name="android:textColor">@color/adressOblakoSecond</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">italic</item>
    </style>
    <style name="OblakoPrice">
        <item name="android:textColor">@color/textColorOblako</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">italic</item>
    </style>
</resources>
